{
    "name": "wrapper",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "rm -rf dist && tsc",
        "start:btc": "node dist/index.btc.js",
        "start:trx": "node dist/index.trx.js",
        "dev:btc": "ts-node src/index.btc.ts",
        "dev:trx": "ts-node src/index.trx.ts",
        "dev:btc:log": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.btc.ts",
        "dev:trx:log": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.trx.ts",
        "dep:btc:log": "docker-compose -f docker-compose.btc.yml up --build -d",
        "dep:trx:log": "docker-compose -f docker-compose.trx.yml up --build -d",
        "dep:node:log": "docker-compose -f docker-compose.node.yml up --build",
        "dep:cluster:log": "docker-compose -f docker-compose.cluster.yml --env-file=.cluster.env up --build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bitcoinjs-lib": "^5.0.0",
        "@types/bs58": "^4.0.1",
        "@types/crypto-js": "^3.1.47",
        "@types/dotenv": "^8.2.0",
        "@types/elliptic": "^6.4.12",
        "@types/mongodb": "^3.5.27",
        "@types/node": "^14.10.1",
        "nodemon": "^2.0.4",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "bitcoinjs-lib": "^5.2.0",
        "bs58": "^4.0.1",
        "coinselect": "^3.1.12",
        "crypto-js": "^4.0.0",
        "dotenv": "^8.2.0",
        "elliptic": "^6.5.3",
        "kafkajs": "^1.13.0",
        "mongodb": "^3.6.2",
        "tronweb": "^3.0.0"
    }
}
